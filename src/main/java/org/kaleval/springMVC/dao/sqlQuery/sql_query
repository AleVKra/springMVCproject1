create table Person(
    id int,
    name varchar,
    age int,
    email varchar
);

insert into  Person values(1, 'Tom', 18, 'tom@mail.ru');
insert into  Person values(2, 'Sam', 15, 'asm@mail.ru');
insert into  Person values(3, 'Tolik', 19, 'tim@mail.ru');
insert into  Person values(4, 'Tilk', 20, 'tpkd@mail.ru');

select *from person